{
  "name": "thecoder/laravel-monolog-telegram",
  "description": "Telegram Handler for Monolog",
  "keywords": [
    "log",
    "laravel",
    "monolog",
    "monolog-telegram",
    "logging",
    "bots",
    "telegram",
    "channels"
  ],
  "homepage": "https://github.com/mbpcoder/monolog-telegram",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Mahdi Bagheri",
      "email": "thecoder.ir@gmail.com",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "TheCoder\\MonologTelegram\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "TheCoder\\MonologTelegram\\Tests\\": "tests"
    }
  },
  "require": {
    "php": "^8.0",
    "ext-curl": "*",
    "ext-mbstring": "*",
    "monolog/monolog": "^1.0|^2.0|^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5|^10.0",
    "mockery/mockery": "^1.5",
    "orchestra/testbench": "^8.0|^9.0",
    "illuminate/support": "^10.0|^11.0",
    "guzzlehttp/guzzle": "^7.0"
  },
  "scripts": {
    "test": "phpunit",
    "test:coverage": "phpunit --coverage-html coverage"
  }
}
